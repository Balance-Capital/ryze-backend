{
  "name": "binary-options-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npx typeorm-ts-node-commonjs migration:run -d src/orm.config.ts && node --optimize_for_size --max-old-space-size=460 --gc_interval=100 dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -d src/orm.config.ts",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm/cli.js migration:generate -d src/orm.config.ts",
    "migration:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create -n new_migration",
    "migration:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/orm.config.ts",
    "migration:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d src/orm.config.ts",
    "migration:create:windows": "npm run typeorm migration:create -n new_migration",
    "migration:generate:windows": "npm run typeorm migration:generate",
    "migration:run:windows": "npm run typeorm migration:run",
    "migration:down:windows": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@apollo/client": "^3.8.5",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.2.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "async-sema": "^3.1.1",
    "axios": "^1.2.0",
    "bcrypt": "^5.1.1",
    "bybit-api": "^3.7.6",
    "cache-manager": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-fetch": "^4.0.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "ethereum-multicall": "^2.17.0",
    "ethers": "^5.7.2",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^6.7.0",
    "graphql": "^16.8.1",
    "helmet": "^6.0.1",
    "newrelic": "^9.10.0",
    "node-binance-api": "^0.13.1",
    "node-sparkline": "^1.2.0",
    "papaparse": "^5.4.1",
    "pg": "^8.8.0",
    "pg-connection-string": "^2.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "short-unique-id": "^4.4.4",
    "socket.io": "^4.5.4",
    "swagger-ui-express": "^4.6.0",
    "ts-retry": "^4.1.1",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.17.1",
    "@types/papaparse": "^5.3.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "nock": "^13.3.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sinon": "^15.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "node_modules",
      "test-config",
      ".util.ts",
      ".gateway.ts"
    ],
    "verbose": true
  }
}
